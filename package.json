{
  "name": "eslint-config-tlvince",
  "version": "0.0.0-development",
  "description": "ESLint rule customisations",
  "main": "index.js",
  "scripts": {
    "lint": "yarn eslint && yarn prettier --check",
    "lint:fix": "yarn eslint --fix && yarn prettier --write",
    "eslint": "eslint '**/*.{js,jsx,ts,tsx,json}'",
    "prettier": "prettier '**/*.{js,jsx,ts,tsx,json,css,md}'",
    "test": "./test.js",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tlvince/eslint-config-tlvince.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "tlvince"
  ],
  "author": "Tom Vincent <npm@tlvince.com> (https://tlvince.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tlvince/eslint-config-tlvince/issues"
  },
  "homepage": "https://github.com/tlvince/eslint-config-tlvince",
  "files": [
    "index.js"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-prettier": "3.1.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "8.3.4",
    "commitlint": "8.3.5",
    "eslint": "6.8.0",
    "husky": "4.2.3",
    "lint-staged": "10.1.1",
    "prettier": "2.0.2",
    "semantic-release": "17.0.4",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "eslint": ">= 6",
    "prettier": ">= 1",
    "typescript": ">= 3"
  }
}
